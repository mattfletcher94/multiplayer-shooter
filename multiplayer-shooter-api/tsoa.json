{
    "entryFile": "src/app.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs": ["src/**/*Controller.ts"],
    "spec": {
      "outputDirectory": "./",
      "specVersion": 3,
      "securityDefinitions": {
        "api_key": {
          "type": "apiKey",
          "name": "access_token",
          "in": "query"
        }
      }
    },
    "routes": {
      "routesDir": "./src/",
      "authenticationModule": "src/middleware/authentication.ts"
    }
  }